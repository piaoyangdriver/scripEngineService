##########this is a configure file of ScriptEngine################


##########Global Items####################

LOG_PATH = scriptengine.log
##########Device List####################

[DEV(BEG)]
  NAME = "AndroidAgent"
  ID = Android
  IPADDR = 127.0.0.1
  IPPORT = 8904
  [CMD(BEG)]
    OPENTOOL a  10000
    CLOSETOOL a  1000
    Relay a  100
    SnapShoot a  1000
    Key a b  100
    Press a  100
    Touch a b c  100
    Tap a b  100
    Trackball a b  100
    TypeString a  100
    Flip a  100
    Wake a  100
  [CMD(END)]
[DEV(END)]


[DEV(BEG)]
  NAME = "PicMatchAgent"
  ID = PICMAT
  IPADDR = 127.0.0.1
  IPPORT = 8899
  [CMD(BEG)]
    OPENTOOL a  100
    CLOSETOOL a  100
    PICMATCH target source x y w h  100
  [CMD(END)]
[DEV(END)]


