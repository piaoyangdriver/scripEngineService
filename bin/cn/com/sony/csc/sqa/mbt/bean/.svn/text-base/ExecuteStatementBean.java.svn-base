package cn.com.sony.csc.sqa.mbt.bean;

import java.io.Serializable;

import cn.com.sony.csc.sqa.mbt.constant.ExecuteStatementStatus;

/**
 * 记录执行器的执行情况
 */

public class ExecuteStatementBean implements Serializable {

	private static final long serialVersionUID = -4234797149500205897L;

	private int executeId; // Execute id
	private int executeTestCaseId; // ExecuteTestCase id
	private int executeStatementId; // ExecuteStatement id
	private String script; // Statement Script
	private ExecuteStatementStatus status;// StatementExecute result
	private long runtime; // runtime
	private String log; // log

	public int getExecuteId() {
		return executeId;
	}

	public void setExecuteId(int executeId) {
		this.executeId = executeId;
	}

	public int getExecuteTestCaseId() {
		return executeTestCaseId;
	}

	public void setExecuteTestCaseId(int executeTestCaseId) {
		this.executeTestCaseId = executeTestCaseId;
	}

	public int getExecuteStatementId() {
		return executeStatementId;
	}

	public void setExecuteStatementId(int executeStatementId) {
		this.executeStatementId = executeStatementId;
	}

	public String getScript() {
		return script;
	}

	public void setScript(String script) {
		this.script = script;
	}

	public ExecuteStatementStatus getStatus() {
		return status;
	}

	public void setStatus(ExecuteStatementStatus status) {
		this.status = status;
	}

	public long getRuntime() {
		return runtime;
	}

	public void setRuntime(long runtime) {
		this.runtime = runtime;
	}

	public String getLog() {
		return log;
	}

	public void setLog(String log) {
		this.log = log;
	}

}
